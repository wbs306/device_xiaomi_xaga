<?xml version="1.0" encoding="utf-8"?>
<permissions>
    <privapp-permissions package="com.dolby.daxappui">
        <!-- Pair with Bluetooth devices -->
        <permission name="android.permission.BLUETOOTH" />
        <!-- Access Bluetooth settings -->
        <permission name="android.permission.BLUETOOTH_ADMIN" />
        <!-- Change your audio settings -->
        <permission name="android.permission.MODIFY_AUDIO_SETTINGS" />
        <!-- Run at startup -->
        <permission name="android.permission.RECEIVE_BOOT_COMPLETED" />
        <!-- Modify or delete the contents of your shared storage -->
        <permission name="android.permission.WRITE_EXTERNAL_STORAGE" />
        <!-- Read phone status and identity -->
        <permission name="android.permission.READ_PHONE_STATE" />
        <!-- Read the contents of your shared storage -->
        <permission name="android.permission.READ_EXTERNAL_STORAGE" />
        <permission name="android.permission.MANAGE_USERS" />
        <!-- Work with QS Tiles -->
        <permission name="android.permission.BIND_QUICK_SETTINGS_TILE" />
        <permission name="android.service.quicksettings.action.QS_TILE" />
        <!-- Extra Permissions from other manifests -->
        <permission name="android.permission.INTERACT_ACROSS_USERS" />
        <permission name="android.permission.BIND_JOB_SERVICE" />
        <permission name="android.permission.VIBRATE" />
        <permission name="android.permission.FOREGROUND_SERVICE" />
        <permission name="android.permission.ACCESS_CHECKIN" />
        <permission name="android.permission.WAKELOCK" />
        <permission name="com.dolby.daxappui.permission.WRITE_MATRIX_SUMMARY" />
        <permission name="com.dolby.daxappui.permission.READ_MATRIX_SUMMARY" />
    </privapp-permissions>
</permissions>
